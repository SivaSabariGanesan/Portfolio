{
  "version": 2,
  "builds": [
    {
      "src": "backend/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/index.js"
    }
  ],
  "functions": {
    "backend/index.js": {
      "maxDuration": 10 // Adjust as needed, up to 60 seconds
    }
  }
}
